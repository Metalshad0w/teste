{
  "swagger": "2.0",
  "info": {
    "title": "Conta Corrente Dados Básicos API",
    "version": "v1"
  },
  "schemes": ["https"],
  "host": "apigatewaydes.bvnet.bv",
  "basePath": "/",
  "definitions": {},
  "paths": {"/v1/atacado/contacorrente/dados-basicos": {"post": {
    "consumes": ["application/json"],
    "produces": ["application/json"],
    "description": "Serviço de negócio que trata os dados básicos de Conta Corrente do Atacado e Private.\nSegurança: OAuth (two-legged). Para maiores detalhes do fluxo OAuth de duas pernas, acesse o link: <a href=\"/documentacao#autenticacaolocal\">Documentação OAuth de duas pernas</a>\n",
    "parameters": [
      {
        "in": "header",
        "name": "trilhaAuditoria.ticket",
        "description": "Identificador de correlação do sistema informado pelo cliente.",
        "type": "string"
      },
      {
        "in": "header",
        "name": "trilhaAuditoria.siglaSistema",
        "description": "Identificador do nome da aplicação informado pelo cliente.",
        "type": "string"
      },
      {
        "in": "header",
        "name": "trilhaAuditoria.loginUsuarioFinal",
        "description": "Identificador do usuário da aplicação cliente.",
        "type": "string"
      },
      {
        "in": "header",
        "name": "trilhaAuditoria.enderecoIpCliente",
        "description": "IP de origem do acesso (aplicado apenas para sistemas Client Server e Sistemas Internet).",
        "type": "string"
      },
      {
        "in": "header",
        "name": "trilhaAuditoria.fase",
        "description": "Sequencial para indicar a ordem da chamada. Incrementada por cada camada de serviço.",
        "type": "integer"
      },
      {
        "in": "body",
        "name": "body",
        "required": true,
        "schema": {
          "$id": "http://example.com/example.json",
          "type": "object",
          "definitions": {},
          "properties": {
            "pessoa": {
              "type": "object",
              "properties": {"codigoPessoa": {"type": "integer"}}
            },
            "documento": {
              "type": "object",
              "properties": {"numeroDocumento": {"type": "string"}}
            },
            "sistema": {
              "type": "object",
              "properties": {
                "codigoPessoaCorrelation": {"type": "integer"},
                "codigoSistemaOrigem": {"type": "integer"}
              }
            }
          }
        }
      }
    ],
    "responses": {
      "200": {
        "description": "Retorno.",
        "schema": {
          "$id": "http://example.com/example.json",
          "type": "object",
          "definitions": {},
          "properties": {"listaContaCorrente": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "contaCorrente": {
                  "type": "object",
                  "properties": {
                    "codigo": {"type": "integer"},
                    "numero": {"type": "string"},
                    "digito": {"type": "string"},
                    "agencia": {
                      "type": "object",
                      "properties": {
                        "codigo": {"type": "integer"},
                        "numero": {"type": "string"},
                        "digito": {"type": "string"},
                        "nome": {"type": "string"},
                        "nomeAbreviado": {"type": "string"}
                      }
                    }
                  }
                },
                "situacaoContaCorrente": {
                  "type": "object",
                  "properties": {
                    "codigo": {"type": "integer"},
                    "descricao": {"type": "string"},
                    "indicadorContaOperante": {"type": "boolean"}
                  }
                },
                "tipoContaCorrente": {
                  "type": "object",
                  "properties": {
                    "codigo": {"type": "integer"},
                    "descricao": {"type": "string"},
                    "tipoContaAbreviado": {"type": "string"}
                  }
                }
              }
            }
          }}
        },
        "examples": {"application/json": {"listaContaCorrente": [
          {
            "contaCorrente": {
              "codigo": 1,
              "numero": "numero",
              "digito": "digito",
              "agencia": {
                "codigo": 1,
                "numero": "numero",
                "digito": "digito",
                "nome": "nome",
                "nomeAbreviado": "nomeAbreviado"
              }
            },
            "situacaoContaCorrente": {
              "codigo": 1,
              "descricao": "descricao",
              "indicadorContaOperante": true
            },
            "tipoContaCorrente": {
              "codigo": 1,
              "descricao": "descricao",
              "tipoContaAbreviado": "tipoContaAbreviado"
            }
          },
          {
            "contaCorrente": {
              "codigo": 1,
              "numero": "numero",
              "digito": "digito",
              "agencia": {
                "codigo": 1,
                "numero": "numero",
                "digito": "digito",
                "nome": "nome",
                "nomeAbreviado": "nomeAbreviado"
              }
            },
            "situacaoContaCorrente": {
              "codigo": 1,
              "descricao": "descricao",
              "indicadorContaOperante": true
            },
            "tipoContaCorrente": {
              "codigo": 1,
              "descricao": "descricao",
              "tipoContaAbreviado": "tipoContaAbreviado"
            }
          }
        ]}}
      },
      "400": {
        "description": "Request Inválido.",
        "schema": {
          "type": "object",
          "properties": {"fault": {
            "type": "object",
            "properties": {
              "codigo": {"type": "string"},
              "tipo": {"type": "string"},
              "sistemaOrigem": {"type": "string"},
              "mensagem": {"type": "string"},
              "servicoOrigem": {"type": "string"},
              "listaExcecao": {
                "type": "object",
                "properties": {"excecao": {
                  "type": "object",
                  "properties": {
                    "nome": {"type": "string"},
                    "descricao": {
                      "type": "array",
                      "items": {"type": "string"}
                    }
                  },
                  "required": [
                    "nome",
                    "descricao"
                  ]
                }},
                "required": ["excecao"]
              }
            },
            "required": [
              "codigo",
              "tipo",
              "sistemaOrigem",
              "mensagem",
              "servicoOrigem"
            ]
          }}
        }
      },
      "401": {
        "description": "Autenticação Inválida"
      },
      "403": {
        "description": "Autorização recusada"
      },
      "500": {
        "description": "Erro do Servidor"
      }
    }
  }}}
}
